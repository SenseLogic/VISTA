MakeOverflowVisible( media_query_name_list = {} )

    .overflow-visible
    {
        overflow: visible;
    }

    for media_query_name in media_query_name_list
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .overflow-visible-{media_query_name}
            {
                overflow: visible;
            }
        }
    }


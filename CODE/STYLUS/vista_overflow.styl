MakeOverflowScrollbarHidden( media_query_name_array = {}, prefix =, suffix =, important = )

    .{prefix}overflow-scrollbar-hidden{suffix}
    {
        -ms-overflow-style: none important;
        scrollbar-width: none important;
    }

    .{prefix}overflow-scrollbar-hidden{suffix}::-webkit-scrollbar
    {
        display: none important;
    }

    for media_query_name in media_query_name_array
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .{prefix}overflow-scrollbar-hidden-{media_query_name}{suffix}
            {
                -ms-overflow-style: none important;
                scrollbar-width: none important;
            }

            .{prefix}overflow-scrollbar-hidden-{media_query_name}{suffix}::-webkit-scrollbar
            {
                display: none important;
            }
        }
    }


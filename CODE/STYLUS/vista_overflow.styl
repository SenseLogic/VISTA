// -- MIXINS

MakeOverflowVisible( media_query_name_array = {}, prefix =, suffix =, important = )

    .{prefix}overflow-visible{suffix}
    {
        overflow: visible important;
    }

    for media_query_name in media_query_name_array
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .{prefix}overflow-visible-{media_query_name}{suffix}
            {
                overflow: visible important;
            }
        }
    }


MakeOverflowScrollbarHidden( media_query_name_list = {} )

    .overflow-scrollbar-hidden
    {
        -ms-overflow-style: none;
        scrollbar-width: none;
    }

    .overflow-scrollbar-hidden::-webkit-scrollbar
    {
        display: none;
    }

    for media_query_name in media_query_name_list
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .overflow-scrollbar-hidden-{media_query_name}
            {
                -ms-overflow-style: none;
                scrollbar-width: none;
            }

            .overflow-scrollbar-hidden-{media_query_name}::-webkit-scrollbar
            {
                display: none;
            }
        }
    }


MakeOverflowVisible( media_query_name_list = {}, suffix = )

    .overflow-visible{suffix}
    {
        overflow: visible;
    }

    for media_query_name in media_query_name_list
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .overflow-visible-{media_query_name}{suffix}
            {
                overflow: visible;
            }
        }
    }


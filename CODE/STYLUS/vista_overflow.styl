MakeOverflowScrollbarHidden( media_query_name_list = {}, suffix = )

    .overflow-scrollbar-hidden{suffix}
    {
        -ms-overflow-style: none;
        scrollbar-width: none;
    }

    .overflow-scrollbar-hidden{suffix}::-webkit-scrollbar
    {
        display: none;
    }

    for media_query_name in media_query_name_list
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .overflow-scrollbar-hidden-{media_query_name}{suffix}
            {
                -ms-overflow-style: none;
                scrollbar-width: none;
            }

            .overflow-scrollbar-hidden-{media_query_name}{suffix}::-webkit-scrollbar
            {
                display: none;
            }
        }
    }


MakeListStyleTypeNone( media_query_name_list = {}, prefix =, suffix = )

    .{prefix}list-style-type-none{suffix}
    {
        list-style-type: none;
    }

    for media_query_name in media_query_name_list
    {
        @media MediaQueryMap[ media_query_name ]
        {
            .{prefix}list-style-type-none-{media_query_name}{suffix}
            {
                list-style-type: none;
            }
        }
    }

